    {
        "id": "c5149faa.77c5d",
        "type": "ui_template",
        "z": "7676309c.b164e",
        "group": "b9cdb163.22e3d",
        "name": "WLANs",
        "order": 1,
        "width": "0",
        "height": "0",
        "format": "<html>\n<head>\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1\">\n    <meta charset=\"utf-8\">\n</head>\n\n<body>\n    <div id=\"renderList\" class=\"nr-dashboard-button ng-scope _md visible\"></div>\n\n</body>\n\n<script>\n    var list = document.getElementById('renderList');\n\n    (function(scope) {\n        scope.$watch('msg.payload', function(data) {\n            ssidList = data;\n            \n            if(Array.isArray(ssidList)){\n                list.innerHTML = '';\n                ssidList.forEach(renderSSIDList);\n            }\n        });\n        \n        function renderSSIDList(element, index, arr) {\n            var bttn = document.createElement('button');\n            bttn.onclick = function(){scope.send({payload: \"\\\"\" + element + \"\\\"\"})};\n            bttn.innerHTML=bttn.innerHTML + element;\n            bttn.classList.add(\"md-button\");\n            \n            list.appendChild(bttn);\n            p=document.createElement('p');\n            list.appendChild(p);\n        }\n        \n    })(scope);\n</script>\n</html>",
        "storeOutMessages": true,
        "fwdInMessages": false,
        "templateScope": "local",
        "x": 800,
        "y": 820,
